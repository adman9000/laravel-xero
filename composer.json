{
    "name": "adman9000/laravel-xero",
    "description": "A Laravel 5 wrapper for Xero's PHP API forked from drawmyattention/xerolaravel",
    "require": {
        "calcinai/xero-php": "^2.2"
    },
    "authors": [
        {
            "name": "Adam Harding"
        }
    ],
    "autoload": {
        "psr-4": {
          "adman9000\\LaravelXero\\": "/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "adman9000\\LaravelXero\\Providers\\XeroServiceProvider"
            ]
        }
    }
}
